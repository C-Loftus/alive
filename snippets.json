{
    "Format String": {
        "prefix": "format",
        "body": ["format ${1:T} \"$2\" $0"]
    },
    "Load File": {
        "prefix": "load",
        "body": ["load \"$1\""]
    },
    "Comment": {
        "prefix": "comment",
        "body": ["#| $0 |#"]
    },
    "Define Function": {
        "prefix": "defun",
        "body": ["defun $1 ($2) $0"]
    }
}
