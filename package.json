{
    "name": "common-lisp",
    "displayName": "Common Lisp",
    "description": "Syntax highlighting for Common Lisp",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./src/extension.js",
    "activationEvents": [
        "onLanguage:common-lisp"
    ],
    "contributes": {
        "languages": [
            {
                "id": "common-lisp",
                "aliases": [
                    "Common Lisp",
                    "common-lisp"
                ],
                "extensions": [
                    ".lisp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "colors": [
            {
                "id": "common_lisp.default",
                "description": "Common Lisp Keyword",
                "defaults": {
                    "light": "#d4d4d4ff",
                    "dark": "#d4d4d4ff",
                    "highContrast": "#d4d4d4ff"
                }
            },
            {
                "id": "common_lisp.white_space",
                "description": "Common Lisp Whitespace",
                "defaults": {
                    "light": "#666",
                    "dark": "#666",
                    "highContrast": "#666"
                }
            },
            {
                "id": "common_lisp.keyword",
                "description": "Common Lisp Keyword",
                "defaults": {
                    "light": "#569cd6ff",
                    "dark": "#569cd6ff",
                    "highContrast": "#569cd6ff"
                }
            },
            {
                "id": "common_lisp.control",
                "description": "Common Lisp Control Words",
                "defaults": {
                    "light": "#c586c0ff",
                    "dark": "#c586c0ff",
                    "highContrast": "#c586c0ff"
                }
            },
            {
                "id": "common_lisp.function",
                "description": "Common Lisp Function",
                "defaults": {
                    "light": "#dcdcaaff",
                    "dark": "#dcdcaaff",
                    "highContrast": "#dcdcaaff"
                }
            },
            {
                "id": "common_lisp.string",
                "description": "Common Lisp String",
                "defaults": {
                    "light": "#ce9178ff",
                    "dark": "#ce9178ff",
                    "highContrast": "#ce9178ff"
                }
            },
            {
                "id": "common_lisp.package",
                "description": "Common Lisp Package",
                "defaults": {
                    "light": "#4ec9b0ff",
                    "dark": "#4ec9b0ff",
                    "highContrast": "#4ec9b0ff"
                }
            },
            {
                "id": "common_lisp.symbol",
                "description": "Common Lisp Symbol",
                "defaults": {
                    "light": "#9cdcfeff",
                    "dark": "#9cdcfeff",
                    "highContrast": "#9cdcfeff"
                }
            },
            {
                "id": "common_lisp.error",
                "description": "Common Lisp Error",
                "defaults": {
                    "light": "#ff0000",
                    "dark": "#ff0000",
                    "highContrast": "#ff0000"
                }
            }
        ]
    }
}
